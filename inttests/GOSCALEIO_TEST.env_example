# Make sure system has a name (not just an ID) for tests to pass
GOSCALEIO_INSECURE=true
GOSCALEIO_HTTP_TIMEOUT=60
GOSCALEIO_ENDPOINT=https://1.2.3.4:443
GOSCALEIO_USERNAME=admin
GOSCALEIO_PASSWORD=Password123
GOSCALEIO_DEBUG=true
GOSCALEIO_PROTECTIONDOMAIN=domain1
GOSCALEIO_STORAGEPOOL=pool1
GOSCALEIO_SYSTEMNAME=powerflex-gateway
GOSCALEIO_INSTALLATIONID=08bhsd7f9cfy890g
GOSCALEIO_SDSID=0db2c37100002311
USER_PASSWORD=Password

# For filesystem operations, enable these values
GOSCALEIO_NASSERVER=env8nasserver
GOSCALEIO_FILESYSTEM=env8FS

# For NFSExport operations , enable these values
GOSCALEIO_NFSEXPORT=nfsexport1
GOSCALEIO_FILESYSTEM_NFSEXPORT=nfsfilesysytem

# For Tree Quota, enable these values
GOSCALEIO_TREEQUOTAID = tree-quota-id

# Introduce a second System for replication test. If not specified, replication tests will be skipped.
GOSCALEIO_ENDPOINT2=https://1.2.3.5:443
GOSCALEIO_USERNAME2=admin
GOSCALEIO_PASSWORD2=Password123
GOSCALEIO_PROTECTIONDOMAIN2=domain1
GOSCALEIO_STORAGEPOOL2=pool1

# Replication volume names. This should be created ahead of time on the arrays.
GOSCALEIO_REPLICATION_SOURCE_NAME=replication-source
GOSCALEIO_REPLICATION_TARGET_NAME=replication-target

# Gateway Variables
GATEWAY_ENDPOINT=https://1.2.3.4:443
GATEWAY_USERNAME=admin
GATEWAY_PASSWORD=Password123
GATEWAY_INSECURE=true